{
  "npmScope": "snowfrog",
  "affected": { "defaultBase": "master" },
  "implicitDependencies": {
    "workspace.json": "*",
    "package.json": { "dependencies": "*", "devDependencies": "*" },
    "tsconfig.base.json": "*",
    "tslint.json": "*",
    ".eslintrc.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": { "cacheableOperations": ["build", "lint", "test", "e2e"] }
    }
  },
  "projects": {
    "girl-friday": { "tags": [] },
    "girl-friday-example-app": { "tags": [] },
    "girl-friday-example-app-e2e": { "tags": [], "implicitDependencies": ["girl-friday-example-app"] }
  },
  "workspaceLayout": { "appsDir": "packages", "libsDir": "packages" }
}

{
  "version": 2,
  "projects": {
    "girl-friday": {
      "root": "packages/girl-friday",
      "sourceRoot": "packages/girl-friday/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "packages/girl-friday/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/packages/girl-friday"
          ],
          "options": {
            "jestConfig": "packages/girl-friday/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "executor": "@nrwl/node:package",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/packages/girl-friday",
            "tsConfig": "packages/girl-friday/tsconfig.lib.json",
            "packageJson": "packages/girl-friday/package.json",
            "main": "packages/girl-friday/src/index.ts",
            "assets": [
              "packages/girl-friday/*.md"
            ]
          }
        }
      }
    },
    "girl-friday-example-app": {
      "projectType": "application",
      "root": "packages/girl-friday/example-app",
      "sourceRoot": "packages/girl-friday/example-app/src",
      "prefix": "snowfrog",
      "targets": {
        "build": {
          "executor": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/packages/girl-friday/example-app",
            "index": "packages/girl-friday/example-app/src/index.html",
            "main": "packages/girl-friday/example-app/src/main.ts",
            "polyfills": "packages/girl-friday/example-app/src/polyfills.ts",
            "tsConfig": "packages/girl-friday/example-app/tsconfig.app.json",
            "aot": true,
            "assets": [
              "packages/girl-friday/example-app/src/favicon.ico",
              "packages/girl-friday/example-app/src/assets"
            ],
            "styles": [
              "packages/girl-friday/example-app/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "packages/girl-friday/example-app/src/environments/environment.ts",
                  "with": "packages/girl-friday/example-app/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "girl-friday-example-app:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "girl-friday-example-app:build:production"
            }
          }
        },
        "extract-i18n": {
          "executor": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "girl-friday-example-app:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "packages/girl-friday/example-app/src/**/*.ts",
              "packages/girl-friday/example-app/src/**/*.html"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/packages/girl-friday/example-app"
          ],
          "options": {
            "jestConfig": "packages/girl-friday/example-app/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "girl-friday-example-app-e2e": {
      "root": "packages/girl-friday/example-app-e2e",
      "sourceRoot": "packages/girl-friday/example-app-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "packages/girl-friday/example-app-e2e/cypress.json",
            "tsConfig": "packages/girl-friday/example-app-e2e/tsconfig.e2e.json",
            "devServerTarget": "girl-friday-example-app:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "girl-friday-example-app:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "packages/girl-friday/example-app-e2e/**/*.{js,ts}"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "generators": {
    "@nrwl/angular:application": {
      "style": "css",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress",
      "strict": true
    },
    "@nrwl/angular:library": {
      "style": "css",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "css"
    }
  }
}